{
  "name": "twitbot",
  "version": "0.2.6",
  "description": "Twitter bots for cli..",
  "main": "./dist/twit.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "prepublish": "npm run build",
    "build": "babel ./lib --out-dir ./dist",
    "test": "xo ./lib"
  },
  "bin": {
    "twitbot": "./bin/twitbot"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c0b41/twitbot.git"
  },
  "keywords": [
    "twitter",
    "bots",
    "cli",
    "follow"
  ],
  "author": "Ayhan <cobaimelan@protonmail.ch> (http://www.github.com/c0b41)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c0b41/twitbot/issues"
  },
  "homepage": "https://github.com/c0b41/twitbot#readme",
  "dependencies": {
    "clor": "^1.0.2",
    "forEachAsync": "^3.0.0",
    "forever": "^0.15.1",
    "haller": "^0.1.1",
    "inquirer": "^0.10.0",
    "libnotify": "^1.0.4",
    "minimist": "^1.1.1",
    "nconf": "^0.7.2",
    "object-assign": "^4.0.1",
    "optimist": "^0.6.1",
    "os-homedir": "^1.0.1",
    "trayballoon": "^1.0.4",
    "twit": "^2.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "rimraf": "^2.5.2",
    "xo": "^0.12.1"
  }
}

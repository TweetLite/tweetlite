{
  "name": "twitbot",
  "version": "0.2.6",
  "description": "Twitter bots for cli..",
  "main": "./dist/twit.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "prepublish": "npm run build",
    "build": "babel ./lib --out-dir ./dist",
    "test": "xo ./lib"
  },
  "bin": {
    "twitbot": "./bin/twitbot"
  },
  "xo": {
    "esnext": true,
    "semicolon":false,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c0b41/twitbot.git"
  },
  "keywords": [
    "twitter",
    "bots",
    "cli",
    "follow"
  ],
  "author": "Ayhan <cobaimelan@protonmail.ch> (http://www.github.com/c0b41)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c0b41/twitbot/issues"
  },
  "homepage": "https://github.com/c0b41/twitbot#readme",
  "dependencies": {
    "cli-spinners": "^0.1.2",
    "clor": "^1.6.0",
    "forever": "^0.15.2",
    "inquirer": "^0.10.1",
    "lodash": "^4.13.1",
    "log-update": "^1.0.2",
    "loud-rejection": "^1.4.1",
    "minimist": "^1.2.0",
    "nconf": "^0.7.2",
    "object-assign": "^4.1.0",
    "optimist": "^0.6.1",
    "os-homedir": "^1.0.1",
    "promise-series2": "^1.1.0",
    "twit": "^2.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.2",
    "xo": "^0.12.1"
  }
}

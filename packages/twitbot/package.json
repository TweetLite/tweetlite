{
  "name": "twitbot",
  "version": "0.3.0-beta1",
  "description": "Twitter bots for cli..",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "rimraf ./lib",
    "prepublish": "npm run build",
    "build": "babel ./src --out-dir ./lib",
    "test": "xo ./lib"
  },
  "bin": {
